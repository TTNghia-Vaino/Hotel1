@model Hotel1.Models.Loaiphong

@{
    ViewData["Title"] = "Details";
}

<h3>Chi tiết</h3>

<div>
    
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Loại phòng
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Malp)
        </dd>
        <dt class="col-sm-2">
            Số lượng
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Soluongtoida)
        </dd>
        <dt class="col-sm-2">
            Giá loại phòng
        </dt>
        <dd class="col-sm-10">
            @String.Format("{0:N0}", Model.Gialoaiphong)
        </dd>
    </dl>

</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Malp">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
